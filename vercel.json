{
    "version": 2,
    "buildCommand": "sed -i \"s|{{API_BASE_URL}}|$API_BASE_URL|g; s|{{NOTIFICATION_API_BASE_URL}}|$NOTIFICATION_API_BASE_URL|g\" public/env.js",
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/.well-known/apple-app-site-association",
        "headers": {
          "Content-Type": "application/json"
        },
        "dest": "/.well-known/apple-app-site-association"
      },
      {
        "src": "/.well-known/assetlinks.json",
        "headers": {
          "Content-Type": "application/json"
        },
        "dest": "/.well-known/assetlinks.json"
      },
      {
        "src": "/order/.*",
        "dest": "/index.html"
      },
      {
        "src": "/resume/.*",
        "dest": "/index.html"
      },
      {
        "src": "/equipment/.*",
        "dest": "/index.html"
      },
      {
        "src": "/company/.*",
        "dest": "/index.html"
      },
      {
        "src": "/sign/callback",
        "dest": "/sign/callback.html"
      },
      {
        "src": "/sign/success",
        "dest": "/sign/success.html"
      },
      {
        "src": "/sign/error",
        "dest": "/sign/error.html"
      },
      {
        "src": "/sign/document/.*",
        "dest": "/sign/docsign.html"
      },

      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
}