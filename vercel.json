{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/order/(.*)",
        "destination": "/order/order.html"
      },
      {
        "source": "/resume/(.*)",
        "destination": "/index.html"
      },
      {
        "source": "/equipment/(.*)",
        "destination": "/index.html"
      },
      {
        "source": "/company/(.*)",
        "destination": "/index.html"
      },
      {
        "source": "/cv/(.*)",
        "destination": "/index.html"
      },
      {
        "source": "/sign/callback",
        "destination": "/sign/callback.html"
      },
      {
        "source": "/sign/success",
        "destination": "/sign/success.html"
      },
      {
        "source": "/sign/error",
        "destination": "/sign/error.html"
      },
      {
        "source": "/sign/document/(.*)",
        "destination": "/sign/docsign.html"
      }
    ],
    "headers": [
      {
        "source": "/.well-known/apple-app-site-association",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      },
      {
        "source": "/.well-known/assetlinks.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ]
}